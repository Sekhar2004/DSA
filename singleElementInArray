1)using xor

      #include<vector>
//done using xor as xor usually while making xor operation a^a=0;
//as we are aiming for single element in given array it returns only single digit.
int getSingleElement(vector<int> &arr)
{	
	int n= arr.size();
	int xorr=0;
	for(int i=0;i<n;i++)
	{
		xorr=xorr^arr[i];


	}
	return xorr;

}


